<odoo>
  <data>

        <!--FORMATO DEL PAPEL PARA CHEQUE-->
        <record id="paperformat_cheque" model="report.paperformat">
        <field name="name">cheque guarani</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">240</field>
        <field name="page_width">76</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
        </record>

        <!--REPORTE CHEQUE-->
        <report
            id="report_cheque_guarani"
            model="account.check"
            string="Cheque Guaranies cvsa"
            name="module_cheque_genetyx.cheque_report_genetyx"
            file="module_cheque_genetyx.cheque_report_genetyx"
            paperformat="paperformat_cheque"
            report_type="qweb-pdf"
                />
      <!--FORMATO PAPEL CHEQUE DOLAR-->
      <record id="paperformat_cheque_dolar" model="report.paperformat">
        <field name="name">cheque formato dolar</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">240</field>
        <field name="page_width">76</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
        </record>

      <!--REPORTE CHEQUE DOLAR-->
            <report
            id="report_cheque_dolar"
            model="account.check"
            string="Cheque Dolar cvsa"
            name="module_cheque_genetyx.cheque_report_dolar_genetyx"
            file="module_cheque_genetyx.cheque_report_dolar_genetyx"
            paperformat="paperformat_cheque_dolar"
            report_type="qweb-pdf"
                />

        </data>
</odoo>